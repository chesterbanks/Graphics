background(255, 255, 255);
noFill();
strokeCap(ROUND);
pushMatrix();
translate(5, 7);
for(var i = 21; i > 0; i -= 2){
    strokeWeight(i);
    stroke(0, 0, 0, 7);
    bezier(193, 127, 127, 86, 70, 213, 182, 200);
    bezier(200, 255, 256, 270, 262, 186, 182, 200);
    bezier(182, 307, 228, 349, 296, 273, 200, 255);
    bezier(182, 307, 171, 264, 192, 257, 177, 222);
}
popMatrix();
strokeWeight(16);
stroke(0, 179, 255);
bezier(193, 127, 127, 86, 70, 213, 182, 200);
bezier(200, 255, 256, 270, 262, 186, 182, 200);
bezier(182, 307, 228, 349, 296, 273, 200, 255);
bezier(182, 307, 171, 264, 192, 257, 177, 222);

for(var i = 16; i > 0; i -= 2){
    strokeWeight(i);
    stroke(255, 255, 255, 20);
    bezier(193, 127, 127, 86, 70, 213, 182, 200);
    bezier(200, 255, 256, 270, 262, 186, 182, 200);
